{
  "modal-trigger#wishlist": {
      "children": [
          "icon-expand",
          "modal-layout#wishlist"
      ],
      "props": {
          "blockClass": "wishlist"
      }
  },
  "icon-expand#wishlist": {
      "props": {
          "blockClass": "wishlist"
      }
  },
  "modal-trigger#wishlist-v2": {
      "children": [
          "rich-text#wishlist",
          "modal-layout#wishlist"
      ],
      "props": {
          "blockClass": "wishlist"
      }
  },
  "rich-text#wishlist": {
      "props": {
        "text": "![wishlist](/arquivos/wish-list.png) **Mi lista**",
        "blockClass": ["wishlist"]
      }
  },
  "modal-layout#wishlist": {
      "children": [
          "modal-header#wishlist",
          "modal-content#wishlist"
      ],
      "props": {
          "blockClass": "wishlist"
      }
  },
  "modal-header#wishlist": {
      "props": {
          "blockClass": "wishlist",
          "showCloseButton": true,
          "iconCloseSize": 32
      }
  },
  "modal-content#wishlist": {
      "children": [
          "add-to-wishlist"
      ],
      "props": {
          "blockClass": "wishlistContent"
      }
  },
  "add-to-wishlist": {
      "children": [
          "login-content#wishlist"
      ]
  },
  "add-to-wishlist-minicart":{
    "children": [
      "login-content#wishlist"
  ]
  },
  "modal-content#wishlistminicart": {
    "children": [
        "add-to-wishlist-minicart"
    ],
    "props": {
        "blockClass": "wishlistContentCart"
    }
},
  "login-content#wishlist": {
      "props": {
        "isInitialScreenOptionOnly": false,
      "defaultOption": 1,
      "showPasswordVerificationIntoTooltip": true,
          "blockClass": "login-wishlist",
          "optionsTitle": "Ingresa a tu cuenta para poder agregar productos a tu lista"
      }
  }
}